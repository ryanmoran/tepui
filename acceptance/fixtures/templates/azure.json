{
  "provider": {
    "azurerm": {
      "subscription_id": "some-subscription-id",
      "client_id": "some-client-id",
      "client_secret": "some-client-secret",
      "tenant_id": "some-tenant-id"
    }
  },
  "resource": {
    "azurerm_resource_group": {
      "resource_group": {
        "name": "some-environment",
        "location": "some-region"
      }
    },
    "azurerm_virtual_network": {
      "some-network-1": {
        "name": "some-network-1",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_space": ["1.2.3.4/5"],
        "location": "some-region"
      },
      "some-network-2": {
        "name": "some-network-2",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_space": ["6.7.8.9/10"],
        "location": "some-region"
      }
    },
    "azurerm_subnet": {
      "some-subnet-1": {
        "name": "some-subnet-1",
        "virtual_network_name": "${azurerm_virtual_network.some-network-1.name}",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_prefix": "2.3.4.5/6"
      },
      "some-subnet-2": {
        "name": "some-subnet-2",
        "virtual_network_name": "${azurerm_virtual_network.some-network-1.name}",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_prefix": "3.4.5.6/7"
      },
      "some-subnet-3": {
        "name": "some-subnet-3",
        "virtual_network_name": "${azurerm_virtual_network.some-network-2.name}",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_prefix": "7.8.9.10/11"
      },
      "some-subnet-4": {
        "name": "some-subnet-4",
        "virtual_network_name": "${azurerm_virtual_network.some-network-2.name}",
        "resource_group_name": "${azurerm_resource_group.resource_group.name}",
        "address_prefix": "8.9.10.11/12"
      }
    }
  }
}
