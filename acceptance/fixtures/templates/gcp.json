{
  "provider": {
    "google": {
      "credentials": "some-credentials",
      "project": "some-project",
      "region": "some-region"
    }
  },
  "resource": {
    "google_compute_network": {
      "some-network-1": {
        "name": "some-network-1"
      },
      "some-network-2": {
        "name": "some-network-2"
      }
    },
    "google_compute_subnetwork": {
      "some-subnet-1": {
        "name": "some-subnet-1",
        "ip_cidr_range": "2.3.4.5/6",
        "network": "${google_compute_network.some-network-1.self_link}"
      },
      "some-subnet-2": {
        "name": "some-subnet-2",
        "ip_cidr_range": "3.4.5.6/7",
        "network": "${google_compute_network.some-network-1.self_link}"
      },
      "some-subnet-3": {
        "name": "some-subnet-3",
        "ip_cidr_range": "7.8.9.10/11",
        "network": "${google_compute_network.some-network-2.self_link}"
      },
      "some-subnet-4": {
        "name": "some-subnet-4",
        "ip_cidr_range": "8.9.10.11/12",
        "network": "${google_compute_network.some-network-2.self_link}"
      }
    }
  }
}
