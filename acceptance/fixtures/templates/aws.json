{
  "provider": {
    "aws": {
      "access_key": "some-access-key",
      "secret_key": "some-secret-key",
      "region": "some-region"
    }
  },
  "resource": {
    "aws_vpc": {
      "some-network-1": {
        "cidr_block": "1.2.3.4/5",
        "tags": {
          "Name": "some-network-1",
          "Environment": "some-environment"
        }
      },
      "some-network-2": {
        "cidr_block": "6.7.8.9/10",
        "tags": {
          "Name": "some-network-2",
          "Environment": "some-environment"
        }
      }
    },
    "aws_subnet": {
      "some-subnet-1": {
        "vpc_id": "${aws_vpc.some-network-1.id}",
        "cidr_block": "2.3.4.5/6",
        "tags": {
          "Name": "some-subnet-1",
          "Environment": "some-environment"
        }
      },
      "some-subnet-2": {
        "vpc_id": "${aws_vpc.some-network-1.id}",
        "cidr_block": "3.4.5.6/7",
        "tags": {
          "Name": "some-subnet-2",
          "Environment": "some-environment"
        }
      },
      "some-subnet-3": {
        "vpc_id": "${aws_vpc.some-network-2.id}",
        "cidr_block": "7.8.9.10/11",
        "tags": {
          "Name": "some-subnet-3",
          "Environment": "some-environment"
        }
      },
      "some-subnet-4": {
        "vpc_id": "${aws_vpc.some-network-2.id}",
        "cidr_block": "8.9.10.11/12",
        "tags": {
          "Name": "some-subnet-4",
          "Environment": "some-environment"
        }
      }
    }
  }
}
